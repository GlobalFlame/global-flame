{
  "cleanUrls": true,
  "rewrites": [
    {
      "destination": "/privacy-policy.html",
      "source": "/privacy-policy"
    },
    {
      "destination": "/sanctuary-charter.html",
      "source": "/sanctuary-charter"
    },
    {
      "destination": "/optin.html",
      "source": "/optin"
    },
    {
      "destination": "/optin.html",
      "source": "/email-optin"
    },
    {
      "destination": "/welcome.html",
      "source": "/welcome"
    },
    {
      "destination": "/terms.html",
      "source": "/terms"
    },
    {
      "destination": "/index.html",
      "source": "/"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
